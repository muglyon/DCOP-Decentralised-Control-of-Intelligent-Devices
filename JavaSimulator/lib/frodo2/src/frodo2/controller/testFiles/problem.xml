<instance>
	<presentation maximize="true" />

	<agents nbAgents="1">
		<agent name="agent1" />
	</agents>

	<domains>
		<domain name="D" />
	</domains>
	
	<variables>
		<variable name="V" domain="D" agent="agent1" />
	</variables>
	
	<relations>
		<relation name="R" arity="2" />
	</relations>
	
	<constraints>
		<constraint scope="V" reference="R" />
	</constraints>
</instance>